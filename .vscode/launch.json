{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (SampleApp)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/SampleApps/SampleApp/bin/Debug/net8.0/SampleApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/SampleApps/SampleApp",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
